{
  "private": true,
  "name": "electroblocks",
  "description": "This is electroblocks video editing for code.",
  "version": "3.0.0",
  "scripts": {
    "dev": "cp env/env.development.ts src/env.ts && sapper dev",
    "build": "cp ./env/env.staging.ts ./src/env.ts && NODE_OPTIONS=--max-old-space-size=4096 sapper export --legacy --entry=\"/ /arduino /code /download /lesson /login /open /project-settings /settings /settings/about /settings/myprofile /settings/privacy-policy /settings/support \"",
    "export": "sapper export --legacy",
    "start": "npm run build && firebase serve",
    "export-local": "firebase emulators:start && cp env/env.development.ts src/env.ts && npm run build",
    "export-staging": "cp env/env.staging.ts src/env.ts && npm run build",
    "export-prod": "cp env/env.prod.ts src/env.ts && npm run build",
    "test": "jest",
    "validate": "svelte-check --ignore src/node_modules/@sapper"
  },
  "dependencies": {
    "@rollup/plugin-json": "^4.1.0",
    "@svgdotjs/svg.draggable.js": "^3.0.2",
    "@svgdotjs/svg.js": "^3.0.16",
    "@svgdotjs/svg.panzoom.js": "^2.1.1",
    "@types/axios": "^0.14.0",
    "@types/dotenv": "^8.2.0",
    "@types/file-saver": "^2.0.1",
    "@types/firebase": "^3.2.1",
    "@types/lodash": "^4.14.157",
    "avrgirl-arduino": "git+https://github.com/noopkat/avrgirl-arduino.git",
    "axios": "^0.21.1",
    "blockly": "^3.20200625.2",
    "compression": "^1.7.1",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "file-saver": "^2.0.2",
    "firebase": "^8.0.1",
    "firebase-admin": "^9.4.1",
    "firebase-functions": "^3.11.0",
    "lodash": "^4.17.19",
    "polka": "^0.5.0",
    "prismjs": "^1.21.0",
    "rollup-plugin-copy": "^3.3.0",
    "sirv": "^0.4.6",
    "sveltestrap": "^3.9.7",
    "sweetalert": "^2.1.2",
    "uuid": "^8.3.1"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/runtime": "^7.0.0",
    "@rollup/plugin-babel": "^5.0.0",
    "@rollup/plugin-commonjs": "^14.0.0",
    "@rollup/plugin-image": "^2.0.5",
    "@rollup/plugin-node-resolve": "^8.0.0",
    "@rollup/plugin-replace": "^2.2.0",
    "@rollup/plugin-typescript": "^6.0.0",
    "@tsconfig/svelte": "^1.0.10",
    "@types/compression": "^1.7.0",
    "@types/jest": "^25.2.3",
    "@types/node": "^14.11.8",
    "@types/polka": "^0.5.1",
    "@wessberg/rollup-plugin-ts": "^1.2.27",
    "concurrently": "^5.3.0",
    "jest": "^26.5.3",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.1.4",
    "postcss-import": "^13.0.0",
    "postcss-load-config": "^3.0.0",
    "rollup": "^2.3.4",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-commonjs": "^9.1.6",
    "rollup-plugin-css-only": "^2.1.0",
    "rollup-plugin-node-resolve": "^4.0.0",
    "rollup-plugin-replace": "^2.0.0",
    "rollup-plugin-svelte": "^6.0.0",
    "rollup-plugin-svg-import": "^1.5.1",
    "rollup-plugin-terser": "^7.0.0",
    "sapper": "^0.28.10",
    "svelte": "^3.29.0",
    "svelte-check": "^1.0.46",
    "svelte-preprocess": "^4.5.2",
    "ts-jest": "^26.4.1",
    "tslib": "^2.0.1",
    "typescript": "^4.0.3"
  }
}
