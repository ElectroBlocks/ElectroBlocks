{
  "private": true,
  "name": "electroblocks",
  "description": "This is electroblocks video editing for code.",
  "version": "3.0.0",
  "scripts": {
    "dev-window": "copy env\\env.development.ts src\\env.ts &&  vite dev",
    "build-window": "copy .\\env\\env.staging.ts .\\src\\env.ts &&  vite build",
    "dev-mac": "cp env/env.development.ts src/env.ts &&  vite dev",
    "build-mac": "cp env/env.staging.ts ./src/env.ts &&  vite build",
    "preview": "vite preview",
    "test": "vitest",
    "test-coverage": "vitest --coverage",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^25.0.8",
    "@svelte-plugins/tooltips": "^3.0.1",
    "@sveltejs/adapter-static": "^3.0.5",
    "@sveltejs/kit": "^2.6.2",
    "@sveltejs/vite-plugin-svelte": "^3.1.2",
    "@sveltestrap/sveltestrap": "^6.2.7",
    "@svgdotjs/svg.draggable.js": "^3.0.2",
    "@svgdotjs/svg.js": "^3.0.16",
    "@svgdotjs/svg.panzoom.js": "^2.1.1",
    "@types/axios": "^0.14.0",
    "@types/dom-serial": "^1.0.6",
    "@types/dotenv": "^8.2.0",
    "@types/file-saver": "^2.0.1",
    "@types/firebase": "^3.2.1",
    "@types/jest": "^25.2.3",
    "@types/lodash": "^4.14.157",
    "@types/serialport": "^10.2.0",
    "@types/web": "^0.0.187",
    "@vitest/coverage-istanbul": "^1.5.3",
    "@vitest/coverage-v8": "^1.5.3",
    "axios": "^1.7.4",
    "blockly": "^10.4.3",
    "blockly-field-color-wheel": "^1.0.5",
    "file-saver": "^2.0.5",
    "firebase": "^10.12.2",
    "highlight.js": "^11.9.0",
    "jest": "^29.7.0",
    "lodash": "^4.17.21",
    "rollup-plugin-copy": "^3.3.0",
    "simple-color-picker": "^1.0.5",
    "svelte": "^4.2.19",
    "svelte-check": "^3.6.0",
    "sweetalert": "^2.1.2",
    "ts-jest": "^29.1.2",
    "tslib": "^2.4.1",
    "typescript": "^5.0.0",
    "uuid": "^8.3.1",
    "vite": "^5.0.0",
    "vitest": "^1.5.2"
  },
  "type": "module",
  "dependencies": {
    "esbuild": "^0.24.0",
    "esptool-js": "^0.5.2",
    "serialport": "^12.0.0",
    "usb": "^2.14.0",
    "web-serial-polyfill": "^1.0.15"
  }
